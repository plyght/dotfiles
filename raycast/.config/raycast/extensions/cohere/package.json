{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cohere",
  "title": "Cohere",
  "description": "Allows you to input your Cohere API key, and get a response from the AI of your choice",
  "icon": "extension-icon.png",
  "author": "plyght",
  "owner": "falcon-eclipse",
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "cohereApiKey",
      "type": "textfield",
      "required": true,
      "title": "Cohere API Key",
      "description": "Get this from Cohere's dashboard"
    }
  ],
  "commands": [
    {
      "name": "cohere",
      "title": "Cohere",
      "subtitle": "Cohere AI",
      "description": "Access Cohere's language models",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.89.0",
    "@raycast/utils": "^1.17.0",
    "cohere-ai": "^7.3.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}